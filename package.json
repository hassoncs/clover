{
  "name": "skia-canvas-wasm-deploy-test-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "app"
  ],
  "scripts": {
    "build": "turbo run build",
    "ios": "devmux ensure ios",
    "ios:device": "cd app && npx expo run:ios --configuration Release --device",
    "web": "devmux ensure web",
    "android": "cd app && npx expo run:android",
    "android:device": "cd app && npx expo run:android --variant release --device",
    "clean": "turbo run clean",
    "deploy": "turbo run deploy",
    "lint": "turbo run lint",
    "svc:status": "devmux status",
    "svc:stop": "devmux stop all --force",
    "svc:attach:ios": "devmux attach ios",
    "svc:attach:web": "devmux attach web"
  },
  "devDependencies": {
    "@chriscode/devmux": "link:/Users/hassoncs/Workspaces/Personal/devmux/devmux-cli",
    "turbo": "^2.3.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "pnpm": {
  }
}
