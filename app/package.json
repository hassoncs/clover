{
  "name": "slopcade",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --port 8085",
    "dev": "node scripts/generate-registry.mjs && expo start --port 8085",
    "dev:watch": "node scripts/generate-registry.mjs --watch & expo start --port 8085",
    "generate:registry": "node scripts/generate-registry.mjs",
    "generate:registry:check": "node scripts/generate-registry.mjs --check",
    "generate:registry:watch": "node scripts/generate-registry.mjs --watch",
    "dev:web": "expo start --web --port 8085",
    "ios": "RCT_METRO_PORT=8085 expo run:ios --port 8085",
    "ios:device": "RCT_METRO_PORT=8085 expo run:ios --configuration Release --device --port 8085",
    "android": "RCT_METRO_PORT=8085 expo run:android --port 8085",
    "doctor": "expo doctor",
    "clean:js": "rimraf node_modules .expo .expo-shared dist web-build",
    "clean:ios": "rimraf ios/Pods ios/build ~/Library/Developer/Xcode/DerivedData",
    "clean:android": "rimraf android/.gradle android/build android/app/build",
    "clean": "pnpm run clean:js && pnpm run clean:ios && pnpm run clean:android",
    "pods": "cd ios && RCT_METRO_PORT=8085 pod install",
    "verify:ios": "pnpm run doctor && pnpm run pods && pnpm run ios",
    "verify:android": "pnpm run doctor && pnpm run android",
    "postinstall": "node node_modules/@borndotcom/react-native-godot/scripts/download-prebuilt.js && echo 'postinstall complete'",
    "deploy": "node ../scripts/export-godot.mjs --once && npx expo export -p web && npx wrangler pages deploy dist/ --project-name=slopcade",
    "build": "node ../scripts/export-godot.mjs --once && expo export -p web",
    "godot:export": "node ../scripts/export-godot.mjs --once",
    "godot:watch": "node ../scripts/export-godot.mjs --watch",
    "godot:check": "node ../scripts/export-godot.mjs --check"
  },
  "dependencies": {
    "@borndotcom/react-native-godot": "^1.0.1",
    "@gorhom/bottom-sheet": "^5.2.8",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/slider": "^5.1.2",
    "@sentry/react-native": "~7.2.0",
    "@slopcade/shared": "workspace:*",
    "@slopcade/theme": "workspace:*",
    "@slopcade/ui": "workspace:*",
    "@supabase/supabase-js": "^2.90.1",
    "@tanstack/react-query": "^5.90.19",
    "@trpc/client": "11.8.1",
    "@trpc/react-query": "^11.8.1",
    "aes-js": "^3.1.2",
    "expo": "^54.0.1",
    "expo-constants": "^18.0.13",
    "expo-haptics": "~15.0.8",
    "expo-linking": "^8.0.8",
    "expo-router": "~6.0.0",
    "expo-secure-store": "^15.0.8",
    "expo-sensors": "^15.0.8",
    "expo-updates": "~29.0.16",
    "expo-web-browser": "^15.0.10",
    "munim-bluetooth-peripheral": "^0.4.3",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-ble-plx": "^3.5.0",
    "react-native-css-interop": "0.2.1",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-get-random-values": "^2.0.0",
    "react-native-purchases": "^9.7.1",
    "react-native-reanimated": "^4.1.6",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-sortable-list": "^0.0.25",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.7.0",
    "react-native-worklets-core": "^1.6.2"
  },
  "private": true,
  "devDependencies": {
    "@types/aes-js": "^3.1.4",
    "@types/node": "^25.0.9",
    "@types/react": "~19.1.17",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "rimraf": "^6.0.0",
    "tailwindcss": "3.4.17",
    "typescript": "~5.9.3",
    "vitest": "^2.1.9"
  }
}
